<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Default">
            <Color x:Key="TopSectionColour" >#349EA7</Color>
            <Color x:Key="ImageCountHighlightColor">Black</Color>
            <Color x:Key="ThemeForegroundTextColor">#FF87F727</Color>
            <Color x:Key="BackgroundColour">#FF171717</Color>
        </ResourceDictionary>
        <ResourceDictionary x:Key="HighContrast">
            <Color x:Key="TopSectionColour" >Teal</Color>
            <Color x:Key="ImageCountHighlightColor" >Black</Color>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Light">
            <Color x:Key="TopSectionColour" >Teal</Color>
            <Color x:Key="ImageCountHighlightColor" >Black</Color>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>
